<div id="artists">
    <% @artists.each do |artist| %>
    <div class="artist">
        <div class="info">
            <h2> <%= link_to artist.name, artist %> </h2>
                <p class="genre"><%= artist.genre%></p>
        </div>
        <%= link_to image_tag(artist.artist_image), artist %>
       </div>
    <% end %>
    

